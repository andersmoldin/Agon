@model Dictionary<string, int>
<h1>The results are in!</h1>
<img src="http://data.whicdn.com/images/187132170/large.gif">
<div class="panel-group">
    <div class="panel panel-default">
        <div class="panel-heading">
            <h4 class="panel-title">
                <a data-toggle="collapse" href="#collapse1">And the winner is....</a>
            </h4>
        </div>
        <div id="collapse1" class="panel-collapse collapse">
            <ul class="list-group">
                @{ bool winner = true;}
                @foreach (var item in Model)
                {
                    if (winner)
                    {
                        <li class="list-group-item"><span class="glyphicon glyphicon-glass" style="pointer-events:none;"></span>&nbsp&nbsp&nbsp @item.Key - @item.Value.ToString()</li>
                        winner = false;
                    }
                    else
                    {

                        <li class="list-group-item"><span class="glyphicon glyphicon-thumbs-down" style="pointer-events:none;"></span>&nbsp&nbsp&nbsp&nbsp @item.Key - @item.Value.ToString()</li>
                    }
                }
            </ul>
            <div class="panel-footer"><button class="btn btn-primary" onclick="location.href='@Url.Action("UserLoggedIn", "Home")'">Return to home page</button></div>
        </div>
    </div>
</div>
